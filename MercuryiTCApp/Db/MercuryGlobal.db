record(bo, "$(P)DISABLE") 
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "0")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

record(dfanout, "$(P)DIS:FAN")
{
	field(DOL, "$(P)DISABLE CP")
	field(OMSL, "closed_loop")
	field(OUTA, "$(DISABLE1)")
	field(OUTB, "$(DISABLE2)")
	field(OUTC, "$(DISABLE3)")
	field(OUTD, "$(DISABLE4)")
	field(OUTE, "$(DISABLE5)")
	field(OUTF, "$(DISABLE6)")
}

record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
}

record(dfanout, "$(P)SIM:FAN")
{
	field(DOL, "$(P)SIM CP")
	field(OMSL, "closed_loop")
	field(OUTA, "$(SIM1)")
	field(OUTB, "$(SIM2)")
	field(OUTC, "$(SIM3)")
	field(OUTD, "$(SIM4)")
	field(OUTE, "$(SIM5)")
	field(OUTF, "$(SIM6)")
}

record(waveform, "$(P)CATALOG:_RAW")
{
    field(DESC, "Name")
    field(DTYP, "stream")
    field(INP,  "@mercuryitc.proto getCatalog $(PORT)")
    field(NELM, "1024")
    field(FTVL, "CHAR")
    field(SCAN, "10 second")
    field(PINI, "YES")
    info(archive, "VAL")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:CATALOG:_RAW")
    field(SDIS, "$(P)DISABLE")
}

record(aSub, "$(P)CATALOG:_PARSE") 
{
    field(SNAM, "mercury_parse_catalog")
    field(INPA, "$(P)CATALOG:_RAW CP MS")
    field(FTA, "CHAR")
    field(NOA, "1024")
      
    field(OUTA, "$(P)CATALOG:DEVICENAMES PP")
    field(FTVA, "STRING")
    field(NOVA, "64")
      
    field(OUTB, "$(P)CATALOG:DEVICETYPES PP")
    field(FTVB, "STRING")
    field(NOVB, "64")
}

record(waveform, "$(P)CATALOG:DEVICENAMES")
{
    field(DESC, "Device names in mercury catalog")
    field(NELM, "64")
    field(FTVL, "STRING")
    info(INTEREST, "MEDIUM")
    info(archive, "VAL")
}

record(waveform, "$(P)CATALOG:DEVICETYPES")
{
    field(DESC, "Device names in mercury catalog")
    field(NELM, "64")
    field(FTVL, "STRING")
    info(INTEREST, "MEDIUM")
    info(archive, "VAL")
}

