record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
}

record(waveform, "$(P)SIM:NAME")
{
	field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
	field(FTVL, "CHAR")
	field(NELM, "256")
}
alias("$(P)SIM:NAME", "$(P)SIM:NAME:SP")
alias("$(P)SIM:NAME", "$(P)SIM:NAME:SP:RBV")

